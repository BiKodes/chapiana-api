[run]
source=src
branch = True
parallel=True
concurrency=multiprocessing
omit = 
        *migrations*,
        *urls*,
        *test*,
        *admin*,
        ./manage.py,
        ./contacts/config/*,
        ./contacts/wsgi.py,
        *__init__*

[html]
directory = coverage

[report]
show_missing=True
